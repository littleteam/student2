<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.team.domain.School" table="school" schema="" catalog="stu">
        <id name="schId">
            <column name="sch_id" sql-type="int" not-null="true"/>
        </id>
        <property name="schName">
            <column name="sch_name" sql-type="varchar" length="45"/>
        </property>
    </class>
</hibernate-mapping>