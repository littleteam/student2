<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.team.domain.Student" table="student" schema="" catalog="stu">
        <id name="stuId">
            <column name="stu_id" sql-type="int" not-null="true"/>
        </id>
        <property name="stuName">
            <column name="stu_name" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="stuSex">
            <column name="stu_sex" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="stuGrade">
            <column name="stu_grade" sql-type="int" not-null="true"/>
        </property>
        <property name="schName">
            <column name="sch_name" sql-type="varchar" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>