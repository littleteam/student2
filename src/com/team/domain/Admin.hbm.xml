<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.team.domain.Admin" table="admin" schema="" catalog="stu">
        <id name="admId">
            <column name="adm_id" sql-type="int" not-null="true"/>
        </id>
        <property name="admName">
            <column name="adm_name" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="admSex">
            <column name="adm_sex" sql-type="varchar" length="45"/>
        </property>
        <property name="schId">
            <column name="sch_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>